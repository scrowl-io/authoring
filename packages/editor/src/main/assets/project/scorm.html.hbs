{{#with . as | data |}}
<html lang="en" dir="ltr">
<head>
  <meta name='viewport' content='width=device-width, initial-scale=1'>
  <title>Scrowl Canvas</title>
  <link rel="stylesheet" href="./scrowl.ui.css">
  <link rel="stylesheet" href="./scrowl.player.css">
  <link rel="stylesheet" href="./scrowl.template-core.css">
  {{#each data.templates as | temp |}}
  <link rel="stylesheet" href="{{temp.css}}">
  {{/each}}
  <script type="text/javascript" src="./react.production.min.js"></script>
  <script type="text/javascript" src="./react-dom.production.min.js"></script>
  <script type="text/javascript" src="./react-bootstrap.min.js"></script>
</head>
<body class="owlui-theme--default">
  <div id="app"></div>
  <script type="text/javascript" src="./scrowl.ui.js"></script>
  <script type="text/javascript" src="./scrowl.template-core.js"></script>
  <script type="text/javascript" src="./scrowl.runtime.js"></script>
  <script type="text/javascript" src="./scrowl.player.js"></script>
  {{#each data.templates as | temp |}}
  <script type="text/javascript" src="{{temp.js}}"></script>
  {{/each}}
  <script src="./index.js"></script>
</body>
</html>
{{/with}}